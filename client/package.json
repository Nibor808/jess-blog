{
  "name": "jess-blog",
  "version": "1.0.0",
  "description": "Jess' Blog",
  "main": "app.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "rimraf": "rimraf dist",
    "dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "copy_images": "cp -R images dist/",
    "copy_app": "cp app.js dist/",
    "prod:build": "npm run rimraf && NODE_ENV=production webpack -p --config ./config/webpack.prod.config.js && npm run copy_images && npm run copy_app",
    "lint": "eslint --quiet --color --config config/.eslintrc.json ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nibor808/jess-blog.git"
  },
  "author": "",
