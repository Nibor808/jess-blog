  "description": "Jess' Blog",
  "main": "app.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "rimraf": "rimraf dist",
    "dev": "NODE_ENV=development node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "copy_images": "cp -R images dist/",
    "copy_app": "cp app.js dist/",
    "copy_package": "cp package.json dist/",
    "prod:build": "npm run rimraf && NODE_ENV=production webpack && npm run copy_images && npm run copy_app && npm run copy_package",
    "lint": "eslint --quiet --color --config config/.eslintrc.json ./"
