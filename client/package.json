  "bugs": {
    "url": "https://github.com/Nibor808/jess-blog/issues"
  },
  "homepage": "https://github.com/Nibor808/jess-blog#readme",
  "dependencies": {
    "axios": "^0.15.3",
    "bootstrap-sass": "^3.3.7",
    "css-loader": "^0.26.4",
    "express": "^4.15.3",
    "file-loader": "^0.10.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.1.1",
    "json-loader": "^0.5.4",
    "node-sass": "^4.5.0",
    "react": "^15.4.2",
    "react-bootstrap-carousel": "^1.0.4",
    "react-dom": "^15.4.2",
    "react-modal": "^1.7.3",
    "react-parallax": "^1.2.6",
